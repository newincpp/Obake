{
  "SharedLib":
   {
     "targetName": "Window",
     "staticLibsDeps": ["Obake"],
     "srcExtension": ".cpp",
     "externIncludes": [],
     "outFolder": "Bin",
     "Windows":
     {
       "headersFolders": ["Win32"],
       "compilerFlags": ["-fms-compatibility-version=19.00"],
       "srcFolders": ["Win32"],
       "externLibs": ["user32.lib"]
     },
     "Linux":
     {
       "externLibs": ["xcb"],
       "compilerFlags": ["-fPIC"],
       "srcFolders": ["Linux"]
     }
   }
}
